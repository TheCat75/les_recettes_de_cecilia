{% extends 'base.html.twig' %}

{% block title %}NutritionalValue index{% endblock %}

{% block body %}
    <h1>NutritionalValue index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for nutritional_value in nutritional_values %}
            <tr>
                <td>{{ nutritional_value.id }}</td>
                <td>
                    <a href="{{ path('app_nutritional_value_show', {'id': nutritional_value.id}) }}">show</a>
                    <a href="{{ path('app_nutritional_value_edit', {'id': nutritional_value.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_nutritional_value_new') }}">Create new</a>
{% endblock %}
